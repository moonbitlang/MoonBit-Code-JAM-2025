// 场景选择函数
pub fn load_scene_image(scene_name: String) -> Array[Byte] {
  match scene_name {
    "cave" => load_cave_scene()
    "lake" => load_lake_scene()
    "valley" => load_valley_scene()
    "maze" => load_maze_scene()
    "qiuchi" => load_qiuchi_scene()
    "village" => load_valley_scene()
    "battle" => load_qiuchi_scene()
    _ => create_default_scene()
  }
}

